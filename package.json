{
  "name": "react-companies",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://www.react-companies.com",
  "engines": {
    "node": ">=8.11.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewtpoe/react-companies.git"
  },
  "scripts": {
    "build": "react-scripts build",
    "build:data-file": "node ./scripts/buildDataFile.js && npm run prettier",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject",
    "postbuild": "react-snap",
    "precommit": "npm run build:data-file && lint-staged",
    "predeploy": "npm run build",
    "prestart": "npm run build:data-file",
    "prettier": "prettier --ignore-path ./.eslintignore --write {*,**/**}.{css,js,json,md}",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom"
  },
  "lint-staged": {
    "*.js": [
      "prettier",
      "git add"
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "polished": "^1.9.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "styled-components": "^3.2.6",
    "styled-components-modifiers": "^1.0.1"
  },
  "devDependencies": {
    "dayjs": "^1.6.6",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.0",
    "prettier": "^1.13.5",
    "ramda": "^0.25.0"
  }
}
